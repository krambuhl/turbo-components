{{#if (eq type "ordered")}}
	<ol {{{classAttrib "list list--ordered" addClass}}}>
		{{{children}}}
	</ol>
{{else if (eq type "unordered")}}
	<ul {{{classAttrib "list list--unordered" addClass}}}>
		{{{children}}}
	</ul>
{{else}}
	<ul {{{classAttrib "list" addClass}}}>
		{{{children}}}
	</ul>
{{/if}}
