{{#if (eq attribs.type "ordered")}}
	<ol {{{classAttrib "list list--ordered" attribs.addClass}}}>
		{{{children}}}
	</ol>
{{else if (eq attribs.type "unordered")}}
	<ul {{{classAttrib "list list--unordered" attribs.addClass}}}>
		{{{children}}}
	</ul>
{{else}}
	<ul {{{classAttrib "list" attribs.addClass}}}>
		{{{children}}}
	</ul>
{{/if}}
